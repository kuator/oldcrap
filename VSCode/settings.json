{

    "terminal.integrated.fontFamily": "monospace, PowerlineSymbols",
    "workbench.colorTheme": "Æ’ - Gruvbox",
    "workbench.iconTheme": "material-icon-theme",
    "keyboard.dispatch": "keyCode",
    "vim.useCtrlKeys": true,

"vim.easymotionMarkerFontFamily":"monospace",
  "eslint.options": { "configFile": "/home/evakuator/.eslintrc.json" },
    "vim.useSystemClipboard": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "k",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.easymotion": true,
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [ "ctrl+h" ],
            "commands": [ "workbench.action.navigateLeft" ]
        },
        {
            "before": [ "ctrl+l" ],
            "commands": [ "workbench.action.navigateRight" ]
        },
        {
            "before": [ "ctrl+k" ],
            "commands": [ "workbench.action.navigateUp" ]
        },
        {
            "before": [ "ctrl+j" ],
            "commands": [ "workbench.action.navigateDown" ]
        },
{
            "before": ["ctrl", "w"],
            "commands": [
                "workbench.action.focusActiveEditorGroup"
            ]
        }

    ],
"editor.lineHighlightBackground": "#1E1E1EAA",
"window.zoomLevel": 0,
}
